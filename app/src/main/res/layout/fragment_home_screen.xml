<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeScreenVM" type="com.guruprasadsapplication.app.modules.homescreen.data.viewmodel.HomeScreenVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearHomeScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnselittwentyfour"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStyleblue_A400">
                <LinearLayout android:id="@+id/linearRowselittwentyfour"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSelitTwentyFour"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.48"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{homeScreenVM.homeScreenModel.txtSelitTwentyFour}"
                        style="@style/txtWorkSansromanbold26" tools:text="@string/lbl_selit24"/>
                    <LinearLayout android:id="@+id/linearRowicons"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_128pxh"
                        android:layout_weight="0.52"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIcons"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icons"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icons"/>
                        <ImageView android:id="@+id/imageVector"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_14pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_white_a700_24x28"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700_24x28"/>
                        <ImageView android:id="@+id/imageVectorOne"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_width="@dimen/_23pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_white_a700_28x23"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700_28x23"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <androidx.appcompat.widget.SearchView
                android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_4"
                android:gravity="center_horizontal"
                android:id="@+id/searchViewGroupTwentyOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:theme="@style/searchViewGroupTwentyOne"
                app:iconifiedByDefault="false"
                app:queryBackground="@android:color/transparent"
                app:queryHint="@string/lbl_search_here" app:searchIcon="@drawable/img_search"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBrowseCategori"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{homeScreenVM.homeScreenModel.txtBrowseCategori}"
                style="@style/txtIntermedium12" tools:text="@string/msg_browse_categori"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnPostAds"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_46pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_483pxv"
                android:layout_width="@dimen/_122pxh"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_17pxh"
                android:paddingStart="@dimen/_18pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@string/lbl_post_ads"
                android:textAllCaps="false"
                style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_post_ads"/>
        </LinearLayout>
    </LinearLayout>
</layout>
