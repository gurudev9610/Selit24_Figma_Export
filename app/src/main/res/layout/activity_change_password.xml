<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="changePasswordVM" type="com.guruprasadsapplication.app.modules.changepassword.data.viewmodel.ChangePasswordVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearChangePassword"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowvector"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleblue_A400">
                <FrameLayout android:id="@+id/frameBackbtn"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_40pxh" style="@style/groupStyleblue_1">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtChangePassword"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_85pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.83"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{changePasswordVM.changePasswordModel.txtChangePassword}"
                    style="@style/txtWorkSansromanmedium22_1" tools:text="@string/lbl_change_password"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_48pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageVectorOne"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_blue_a400"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_a400"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOldPassword"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_weight="0.87" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{changePasswordVM.changePasswordModel.txtOldPassword}"
                style="@style/txtWorkSansromanregular18_1" tools:text="@string/lbl_old_password"/>
        </LinearLayout>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_border_blue_a400_radius_4"
            android:drawableEnd="@drawable/img_vector_black_900"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end"
            android:hint="@string/msg_enter_old_passw"
            android:id="@+id/etEmailValue"
            android:inputType="textPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_16pxh"
            android:paddingStart="@dimen/_24pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@={changePasswordVM.changePasswordModel.etEmailValueValue}"
            android:textColorHint="@color/black_900_7f"
            style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearPassword1"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageVectorTwo"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_blue_a400"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_a400"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCreateNewPass"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_weight="0.91" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{changePasswordVM.changePasswordModel.txtCreateNewPass}"
                style="@style/txtWorkSansromanregular18_1" tools:text="@string/msg_create_new_pass"/>
        </LinearLayout>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_border_blue_a400_radius_4"
            android:drawableEnd="@drawable/img_vector_black_900"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end"
            android:hint="@string/msg_enter_new_passw"
            android:id="@+id/etEmailValueOne"
            android:inputType="textPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_16pxh"
            android:paddingStart="@dimen/_24pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@={changePasswordVM.changePasswordModel.etEmailValueOneValue}"
            android:textColorHint="@color/black_900_7f"
            style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearPassword2"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageVectorThree"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_blue_a400"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_blue_a400"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtConfirmPasswor"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_weight="0.9" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{changePasswordVM.changePasswordModel.txtConfirmPasswor}"
                style="@style/txtWorkSansromanregular18_1" tools:text="@string/msg_confirm_passwor"/>
        </LinearLayout>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_border_blue_a400_radius_4"
            android:drawableEnd="@drawable/img_vector_black_900"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end"
            android:hint="@string/msg_enter_password"
            android:id="@+id/etPasswordValue"
            android:inputType="textPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_18pxh"
            android:paddingStart="@dimen/_22pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@={changePasswordVM.changePasswordModel.etPasswordValueValue}"
            android:textColorHint="@color/black_900_7f"
            style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSubmit"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_267pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@string/lbl_submit"
            android:textAllCaps="false" style="@style/btnSolidRounded_2" tools:text="@string/lbl_submit"/>
    </LinearLayout>
</layout>
